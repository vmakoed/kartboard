<%= turbo_frame_tag "participants_list_#{match.code}" do %>
  <h2>Participants:</h2>
  <% if match.participants.empty? %>
    <p>No participants yet</p>
  <% else %>
    <% match.participants.each do |participant| %>
      <p><%= participant.user.name %></p> <!-- replace with your actual attribute -->
    <% end %>
  <% end %>
<% end %>
